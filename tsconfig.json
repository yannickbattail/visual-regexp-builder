{
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "strict": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "Node"
  },
  "lib": ["dom", "es2017"],
  /*"files": ["./model/*.ts"],*/
  "exclude": ["node_modules", "tmp"]
}
